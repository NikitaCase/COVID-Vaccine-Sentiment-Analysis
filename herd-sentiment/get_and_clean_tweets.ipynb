    "# Makes tweets more readable \n",
    "def CleanTweets(txt): \n",
    "    # remove @ from username\n",
    "    txt = txt.replace('@','')   \n",
    "    \n",
    "    # remove RT    \n",
    "    txt = re.sub(r'RT[\\s]+','',txt)\n",
    "    \n",
    "    # remove # but leave txt\n",
    "    txt = re.sub(r'#','', txt)\n",
    "    \n",
    "    # remove hyperlinks\n",
    "    txt = re.sub(r'https?:\\/\\/\\S+', '', txt)\n",
    "    \n",
    "    txt = re.sub('\\n','', txt)\n",
    "    return txt"
   ]
